{
  "rewrites": [
    { "source": "/src/api/waste-collection", "destination": "/src/api/wasteCollectionCalendar.js" },
    { "source": "/", "destination": "/public/index.html" }
  ],
  "crons": [{ "path": "/src/api/waste-collection", "schedule": "0 12 * * *" }],
  "functions": {
    "api/wasteCollectionCalendar.js": {
      "includeFiles": "node_modules/@sparticuz/chromium/**",
      "maxDuration": 60
    }
  }
}
